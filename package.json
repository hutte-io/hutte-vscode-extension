{
  "name": "hutte",
  "displayName": "Hutte",
  "description": "This extension enables to interact with Hutte from VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
		"*"
	],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hutte.login",
        "title": "Login",
        "category": "Hutte"
      },
      {
        "command": "hutte.getOrgFromPool",
        "title": "Get Org From Pool",
        "category": "Hutte"
      },
      {
        "command": "hutte.getOrg",
        "title": "Get Org",
        "category": "Hutte"
      },
      {
				"command": "hutteOrgs.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "hutteOrgs.takeFromPool",
				"title": "Take From Pool"
			},
			{
				"command": "hutteOrgs.authorize",
				"title": "Authorize",
				"icon": {
					"light": "resources/light/login.svg",
					"dark": "resources/dark/login.svg"
				}
			},
			{
				"command": "hutteOrgs.openOnHutte",
				"title": "Open on Hutte"
			}
    ],
    "viewsContainers": {
			"activitybar": [
				{
					"id": "package-explorer",
					"title": "Hutte",
					"icon": "media/Hutte_Logo_Flat.svg"
				}
			]
		},
		"views": {
			"package-explorer": [
				{
					"id": "hutteOrgs",
					"name": "Project view",
					"icon": "media/Hutte_Logo_Flat.svg",
					"contextualTitle": "Hutte"
				}
			]
		},
    "menus": {
			"view/title": [
				{
					"command": "hutteOrgs.refreshEntry",
					"when": "view == hutteOrgs",
					"group": "navigation"
				},
				{
					"command": "hutteOrgs.takeFromPool",
					"when": "view == hutteOrgs"
				}
			],
			"view/item/context": [
				{
					"command": "hutteOrgs.authorize",
					"when": "view == hutteOrgs",
					"group": "inline"
				},
				{
					"command": "hutteOrgs.openOnHutte",
					"when": "view == hutteOrgs"
				}
			]
		}
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "execa": "5.1.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
